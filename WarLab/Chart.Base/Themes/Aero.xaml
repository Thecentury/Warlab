			<GradientStop Color="#FF9BDDFB"
                          Offset="1"/>
		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="ListItemSelectedHoverFill"
                         StartPoint="0,0"
